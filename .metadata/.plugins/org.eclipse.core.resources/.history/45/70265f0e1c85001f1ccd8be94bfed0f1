package co.edu.uco.PrubaPatrones.EjecucionMainFactoryDAO;

import co.edu.uco.PrubaPatrones.EjecucionMainFactoryDAO.ModeloFD.Medico;
import co.edu.uco.PrubaPatrones.EjecucionMainFactoryDAO.ModeloFD.Persona;

public class MedicoDAO implements PersonaDAO {

	@Override
	public void guardar(Persona persona) {
		// Lógica para guardar el médico en la base de datos
        System.out.println("Guardando medico en la base de datos: " + persona.getDescripcion());
		// TODO Auto-generated method stub
		
	}

	@Override
	public Persona obtenerPorId(int id) {
		// Lógica para recuperar un médico por su ID
        return new Medico("Dr. Perez", 45, "Cardiología");
		// TODO Auto-generated method stub
	}
	

}
